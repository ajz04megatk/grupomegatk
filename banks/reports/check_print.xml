<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <template id="check_print">
      <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
          <t t-call="banks.chek_report_banks" />
        </t>
      </t>
    </template>

    <template id="chek_report_banks">
        <div style="display: block; height: 100%;width: 100%; margin-left:-116px; margin-top: -20px;">
          <br/><br/><br/><br/><br/><br/><br/><br/><br/><br/><br/><br/><br/><br/>          
          <div style="font-family: Arial; font-size: 20px; font-weight: bold; height: 370px; width: 750px; -ms-transform: rotate(90deg); transform: rotate(90deg); -webkit-transform: rotate(90deg);-moz-transform: rotate(90deg); -o-transform: rotate(90deg);">
            <div style ='width: 60%; float:right; padding: 20px;text-align: right;'>
              <br/>
              Tegucigalpa M.D.C. <span t-field="o.fecha_string" />
            </div>
           <br/>
            <br/><br/>
            <div style="margin-left: -80px;">
              <table style="width: 100%;">
                <tr>
                  <td style="text-align: center; width: 65%; padding: 30px; font-family: Arial; font-size: 20px; font-weight: bold; text-transform:uppercase;">
                    <span t-field="o.name" />
                  </td>
                  <td style="text-align: center; width: 35%; font-family: Arial; font-size: 20px; font-weight: bold;">
                    &amp;nbsp;&amp;nbsp;&amp;nbsp;<span t-esc="o.total" t-options='{"widget": "float", "precision": 2}' />
                  </td>
                </tr>
                <tr>
                  <td colspan="2" style="margin-left:-10px;font-family: Arial; font-size: 18px; font-weight: bold;">
                    &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;<span t-field="o.amount_total_text" />
                  </td>
                </tr>
                
              </table>
            </div>
          </div>
        </div>  
    </template>
  </data>
</odoo>

